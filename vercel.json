{
    "version": 2,
    "builds": [
      {
        "src": "proyecto/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "50mb", "runtime": "python3.12.2"}
      },
      {
        "src": "staticfiles/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "proyecto/wsgi.py"
      }
    ]
  }